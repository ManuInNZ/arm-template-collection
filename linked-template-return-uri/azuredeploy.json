{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {
        "sas-token": "",
        "artifacts": "[deployment().properties.templateLink.uri]",
        "linked-template": "[uri(variables('artifacts'), concat('/artifacts/azuredeploy-aci.json', variables('sas-token')))]"
    },
    "resources": [
        {
            "name": "vote-database",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/neilpeterson/arm-template-collection/master/linked-template-return-uri/azuredeploy-aci.json",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ],
    "outputs": {
        "output1": {
           "type": "string",
           "value": "[variables('linked-template')]"
        }
    }
}