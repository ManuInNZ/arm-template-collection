{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"_sasToken": {
			"type": "string",
			"defaultValue": ""
		}
	},
	"variables": {},
	"resources": [{
		"name": "vote-database",
		"type": "Microsoft.Resources/deployments",
		"apiVersion": "2018-05-01",
		"properties": {
			"mode": "Incremental",
			"templateLink": {
				"uri": "[concat(uri(deployment().properties.templateLink.uri, 'artifacts/azuredeploy-aci.json'), parameters('_sasToken'))]",
				"contentVersion": "1.0.0.0"
			}
		}
	}]
}