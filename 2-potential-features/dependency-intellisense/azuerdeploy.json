{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "variables": {
        "name": "nepeterstwo"
    },
    "resources": [
        {
            "name": "nepetersone",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2015-06-15",
            "location": "[resourceGroup().location]",
            "properties": {
                "accountType": "Standard_LRS"
            }
        },
        {
            "name": "[variables('name')]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2015-06-15",
            "location": "[resourceGroup().location]",
            "properties": {
                "accountType": "Standard_LRS"
            }
        },
        {
            "name": "nepetersthree",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2015-06-15",
            "location": "[resourceGroup().location]",
            "properties": {
                "accountType": "Standard_LRS"
            },
            "dependsOn": [
                // Can we offer a ctl-space option for precreated dependencies
                // So for this template, ctl-space would pop up this list
                "nepetersone",
                "[variables('name')]",

                // When multiple resources are detected with identical names, offer up using the resourceId
                "[resourceId('Microsoft.Storage/storageAccounts', 'nepetersone')]",
                "[resourceId('Microsoft.Storage/storageAccounts', variables('name'))]"
            ]
        }
    ]
}