{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "variables": {
        "template": "https://raw.githubusercontent.com/neilpeterson/arm-template-collection/master/1-priority-work-examples/linked-template/azuredeploy-aci.json"
    },
    "resources": [
        {
            "name": "vote-frontend",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('template')]",
                    "contentVersion": "1.0.0.0"
                }
            }
        }
    ]
}