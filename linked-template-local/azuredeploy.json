{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "variables": {
        "sas-token": "",
        "artifacts": "[deployment().properties.templateLink.uri]",
        "linked-template": "[uri(variables('artifacts'), concat('/artifacts/azuredeploy-aci.json', variables('sas-token')))]"
    },
    "resources": [
        {
            "name": "vote-frontend",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('linked-template')]"
                }
            }
        }
    ]
}